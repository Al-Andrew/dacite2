# Comprehensive test for string and char literals with all supported escape sequences

fun test_all_escapes : () -> u8 = {
    # Test all string escape sequences
    let newline: u32 = "line1\nline2";
    let carriage: u32 = "text\rmore";
    let null_term: u32 = "text\0end";
    let tab: u32 = "col1\tcol2";
    let backslash: u32 = "path\\to\\file";
    let quote: u32 = "She said \"Hello\"";
    let combined: u32 = "tab\there\nnewline\0null\\slash";
    
    # Test all char escape sequences
    let ch_newline: u32 = '\n';
    let ch_carriage: u32 = '\r';
    let ch_null: u32 = '\0';
    let ch_tab: u32 = '\t';
    let ch_backslash: u32 = '\\';
    let ch_quote: u32 = '\'';
    
    # Test regular chars
    let ch_a: u32 = 'a';
    let ch_Z: u32 = 'Z';
    let ch_0: u32 = '0';
    let ch_9: u32 = '9';
    let ch_space: u32 = ' ';
    let ch_exclaim: u32 = '!';
    
    return 0;
};
